<krpano >

	<!-- include the corfu tour -->
	<include url="../demotour-corfu/tour.xml" />

    <skin_settings i18n="true"
	               tooltips_buttons="true"
	               tooltips_thumbs="true"
	               tooltips_mapspots="true"
				 /> 

    <!-- Multilingual support for krpano viewer = internationalization = i18n -->
