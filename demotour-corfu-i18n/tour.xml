    <!-- Dynamic skin selector in the contextmenu.
        or set the initvars:{design:"design_117"} in the tour.html
        Available values:  
            design_flat_light, design_glass, design_ultra_light, design_117, design_117round, design_black.
        -->
    <include url="vtourskin_selector.xml"  />


	

</krpano>
